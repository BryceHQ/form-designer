{
  "name": "react-hot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "start": "node server.js",
    "build": "webpack --config webpack.production.config.js",
    "release_comment": "-p(压缩js) set NODE_ENV=production(去除react中的warning)",
    "release": "set NODE_ENV=production && webpack -p --config webpack.production.config.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-hot-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaearon/react-hot-boilerplate/issues"
  },
  "homepage": "https://github.com/gaearon/react-hot-boilerplate",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "css-loader": "^0.23.1",
    "es3ify-webpack-plugin": "0.0.0",
    "eslint-plugin-react": "^3.6.2",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "react-hot-loader": "^1.3.0",
    "source-map": "^0.5.6",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1",
    "webpack-sources": "^0.1.2"
  },
  "dependencies": {
    "base64-js": "^1.1.2",
    "classnames": "^2.2.3",
    "flux": "^2.1.1",
    "jquery": "^2.2.3",
    "js-base64": "^2.1.9",
    "lodash": "^4.11.1",
    "material-ui": "^0.14.4",
    "react": "^0.14.8",
    "react-datepicker": "^0.27.0",
    "react-dom": "^0.14.8",
    "react-motion": "^0.4.2",
    "react-router": "^2.2.2",
    "react-tap-event-plugin": "^0.2.1"
  }
}
